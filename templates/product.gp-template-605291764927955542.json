        "ggGx-TT1iRq_text": "<p>1,242 reviews from happy customers</p>",
        "ggIvsbCH1Ro_displayContent": "{percent_discount} OFF",
        "gg7Ji5m4IVN_text": "<p>Crafted from 100% long-staple cotton, our Luxe Cotton Sheet Bundle brings hotel-level softness and breathability to your bedroom.</p>",
        "ggWR2B-9GZh_childItem_0": "<p>1 Top Sheet</p>",
        "ggqrnPpHB7y_childItem_0": "<p>1 Pillowcase Set</p>",
        "gg58HLXHCtb_childItem_0": "<p>1 Fitted Sheet</p>",
        "ggMN7pyp35U_label": "Add to cart",
